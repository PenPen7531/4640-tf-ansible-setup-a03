server {
        listen 80 default_server;
        listen [::]:80 default_server;

        root /home/a02/web_root;

        index index.html index.htm index.nginx-debian.html;

        server_name _;

        location / {
                
                try_files $uri $uri/ =404;
	    
        }

        location /json {
                
                proxy_pass http://{{ backend_priv_ip }}:5000;
	    
        }

}